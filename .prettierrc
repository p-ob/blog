{
	"printWidth": 120,
	"overrides": [
		{
			"files": "*.ts",
			"options": {
				"htmlWhitespaceSensitivity": "strict"
			}
		},
		{
			"files": "*.scss,*.css",
			"options": {
				"parser": "scss",
				"singleQuote": true,
				"printWidth": 200
			}
		},
		{
			"files": "*.md",
			"options": {
				"printWidth": 200
			}
		}
	]
}
